{
  "name": "discord-bot-boilerplate",
  "version": "0.1.0",
  "description": "discord bot",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "build": "npm run clean && tsc && npm run build:cp",
    "build:cp": "cp src/index dist; cp ./{tsconfig-paths-bootstrap.js,tsconfig.json} dist",
    "start": "node dist/index",
    "clean": "rimraf dist/",
    "lint": "eslint --ext .ts --ext .tsx src/"
  },
  "keywords": [],
  "author": "Sander Vispoel <contact@sandervispoel.com>",
  "license": "ISC",
  "dependencies": {
    "@types/node-fetch": "2.5.8",
    "discord.js": "12.5.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "node-fetch": "2.6.1",
    "ts-toolbelt": "9.3.12",
    "tsconfig-paths": "3.9.0"
  },
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/lodash": "4.14.168",
    "@types/moment": "2.13.0",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.20.0",
    "eslint-plugin-import": "2.22.1",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.2",
    "typescript-eslint-parser": "22.0.0"
  }
}
