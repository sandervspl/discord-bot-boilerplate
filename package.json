{
  "name": "discord-bot-boilerplate",
  "version": "0.1.0",
  "description": "discord bot",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node-dev --rs --notify --respawn --transpile-only --project tsconfig.dev.json -r tsconfig-paths/register src/core/main.ts",
    "build": "rm -rf dist/ && rollup -c",
    "start": "node dist/main.js",
    "lint": "eslint --ext .ts --ext .tsx src/"
  },
  "keywords": [],
  "author": "Sander Vispoel <contact@sandervispoel.com>",
  "license": "ISC",
  "dependencies": {
    "discord.js": "12.5.1",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "lodash": "4.17.21",
    "moment": "2.29.1",
    "node-fetch": "2.6.1",
    "reflect-metadata": "0.1.13",
    "ts-toolbelt": "9.3.12",
    "tsconfig-paths": "3.9.0",
    "typedi": "0.10.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-dynamic-import-vars": "1.1.1",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "11.1.1",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "8.1.1",
    "@types/dotenv": "8.2.0",
    "@types/express": "4.17.11",
    "@types/lodash": "4.14.168",
    "@types/moment": "2.13.0",
    "@types/node": "14.14.31",
    "@types/node-fetch": "2.5.8",
    "@typescript-eslint/eslint-plugin": "4.15.2",
    "@typescript-eslint/parser": "4.15.2",
    "eslint": "7.20.0",
    "eslint-plugin-import": "2.22.1",
    "node-notifier": "9.0.0",
    "rollup-plugin-typescript-paths": "1.2.2",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.2",
    "typescript-eslint-parser": "22.0.0"
  }
}
